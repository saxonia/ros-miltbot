<launch>
 <!-- Setup the Configuration of ROBOT -->
    <arg name="ns" default="" />
    <node pkg="tf" type="static_transform_publisher" name="base_link_to_laser_broadcaster" args="0.12 0 0.1 0 0 0 1 $(arg ns)/base_link $(arg ns)/laser 100" />
    <node pkg="tf" type="static_transform_publisher" name="base_footprint_to_base_link" args="0 0 0.12 0 0 0 1 $(arg ns)/base_footprint $(arg ns)/base_link 100" />
    <!--<node pkg="tf" type="static_transform_publisher" name="base_link_to_laser_broadcaster" args="0.12 0 0.1 0 0 0 1 base_link laser 100" />-->
    <!--<node pkg="tf" type="static_transform_publisher" name="base_footprint_to_base_link" args="0 0 0.12 0 0 0 1 base_footprint base_link 100" />-->
    <node pkg="tf" type="static_transform_publisher" name="base_link_to_imu_base_broadcaster" args="0.05 0 0.16 0 0 0 1 $(arg ns)/base_link $(arg ns)/laser 100" />

</launch>